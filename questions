import streamlit as st

st.title("Who Do You Love More? ðŸ‘¶ðŸ§’")

# Dropdown or radio button for user input
choice = st.radio(
    "Choose one:",
    ["Baby Girl", "Baby Boy"]
)

# Display result based on selection
if choice == "Baby Girl":
    st.write("You chose Baby Girl ðŸ‘¶")
elif choice == "Baby Boy":
    st.write("You chose Baby Boy ðŸ§’")
